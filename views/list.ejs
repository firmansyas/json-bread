  <!DOCTYPE html>
  <html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
    <h1> <center>  <%=header %> </h1> </center>
    <style>
        th, td {
      padding: 5px;
      text-align: center;
    }

    .button3{
      background-color: blue;
      border: solid;
      border-width: 5px;
      color: white;
      padding: 10px 30px;
      text-align: center;
      font-size: 10px
      text-decoration:none;
    }

    .button1{
      background-color: green;
      border: solid;
      border-width: 5px;
      color: white;
      text-align: center;
      font-size: 20px;
      text-decoration:none;
      font-family:
    }

    .button2{
      background-color: red;
      border: solid;
      border-width: 5px;
      color: white;
      text-align: center;
      font-size: 20px;
      text-decoration:none;
    }
  </style>
  </head>
  <body>


  <table border= "1">
  <thead>
      <tr>
        <th width="300px">ID</th>
        <th width="300px">String</th>
          <th width="300px">integer</th>
        <th width="300px">Float</th>
        <th width="300px"> Date </th>
        <th width="300px">Boolean </th>
        <th width="300px"> Action </th>
      </tr>
    </thead>

    <tbody>
      <% if (data.length > 0) { %>
        <% for (var i=0; i< data.length; i++){ %>
      <tr>
        <td> <%= data[i].id %></td>
        <td> <%= data[i].string %> </td>
        <td> <%= data[i].integer %> </td>
        <td> <%= data[i].float %> </td>
        <td> <%= data[i].date %> </td>
        <td> <%= data[i].boolean %> </td>
                <td><a href='edit/<%= data[i].id %>' class="button1"> Edit </a> &nbsp;
                <a href= 'delete/<%= data[i].id%>' class ="button2" onclick="return confirm('Are you sure you want to delete this item?');" > Delete </button></td>
      </tr>
      <% } %>
      <% }else { %>
          <tr>
            <td colspan="7"> Data kosong </td>
          </tr>
          <% } %>
        </tbody>
  </table>
  <br> <a href="add" class="button3"> Tambah </a>

  </body>

  </html>
